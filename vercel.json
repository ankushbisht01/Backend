{
  "builds": [
    { 
      "src": "auth/wsgi.py", 
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    { 
      "src": "/(.*)", 
      "dest": "auth/wsgi.py" 
    }
  ],
  "buildCommand": "./build.sh"
}
